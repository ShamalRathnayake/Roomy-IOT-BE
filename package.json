{
  "name": "roomy",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "vitest run",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write .",
    "lint-staged": {
      "*.ts": [
        "eslint --fix",
        "prettier --write"
      ]
    },
    "dev": "ts-node-dev --respawn --transpile-only --poll src/main.ts",
    "build": "tsc",
    "start": "node dist/main.js",
    "d-dev": "docker-compose -f docker-compose.yml -f docker-compose.dev.yml --env-file .env.dev up --build -d && docker-compose logs -f app",
    "d-prod": "docker-compose --env-file .env.prod up --build --no-cache --force-recreate -d  && docker-compose logs -f app",
    "d-prod-c": "docker-compose down -v --remove-orphans && docker-compose --env-file .env.prod build --no-cache && docker-compose --env-file .env.prod up -d --force-recreate && docker-compose logs -f app",
    "prepare": "node .husky/install.mjs"
  },
  "author": "ShamalRathnayake",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@sendgrid/mail": "^8.1.5",
    "@types/socket.io": "^3.0.1",
    "axios": "^1.10.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "compression": "^1.8.0",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.0",
    "form-data": "^4.0.4",
    "helmet": "^8.1.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.16.0",
    "morgan": "^1.10.0",
    "mqtt": "^5.14.0",
    "multer": "^2.0.1",
    "pdf-parse": "^1.1.1",
    "pino": "^9.7.0",
    "pino-pretty": "^13.0.0",
    "socket.io": "^4.8.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.29.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/multer": "^1.4.13",
    "@types/node": "^24.0.3",
    "@types/pdf-parse": "^1.1.5",
    "@types/supertest": "^2.0.16",
    "@vitest/ui": "^3.2.4",
    "cross-env": "^7.0.3",
    "eslint": "^9.29.0",
    "eslint-plugin-vitest": "^0.5.4",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "supertest": "^6.3.4",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.20.3",
    "typescript-eslint": "^8.34.1",
    "vitest": "^3.2.4"
  }
}
